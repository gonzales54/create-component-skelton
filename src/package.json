{
  "name": "create-component-skelton",
  "version": "0.0.5",
  "description": "This is a command to create React component files.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npx tsc --build",
    "prepare": "npm run build",
    "create:component": "ts-node --esm ./bin/index.ts",
    "format": "eslint --fix './bin/**/*.{ts,tsx}' && prettier --write './bin/**/*.{ts,tsx}'"
  },
  "bin": {
    "create-component-skelton": "./dist/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^18.16.3",
    "@types/react": "^18.2.0",
    "create-component-skelton": "^0.0.4",
    "react": "^18.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "prettier": "^2.8.8"
  }
}
